[user]
#  email = atom.powers@gmail.com
  email = atom.r.powers@disney.com
  name = Atom Powers
[push]
  default = current
[core]
  pager = less -r
[branch]
  autosetuprebase = always
[alias]
  br = checkout -b
  ci = commit --all -m
  cin = commit --all --no-verify -m
  co = checkout
  rb = rebase master
  st = status -s
  lg = log --name-status
  ad = add -A
  rbc = rebase --continue
  sq = rebase -i master

